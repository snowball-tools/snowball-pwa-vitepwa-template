  "features": ["Cross Platform", "fast", "simple"],
  "categories": ["social"],
  "shortcuts": [
    {
      "name": "Snowball Tools",
      "short_name": "Snowball Tools",
      "description": "Snowball Tools",
      "url": "/",
      "icons": [
        {
          "src": "assets/icons/icon_512.png",
          "sizes": "512x512",
          "type": "image/png",
          "purpose": "maskable any"
        }
      ]
    }

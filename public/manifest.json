      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "assets/icons/icon_192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "screenshots": [
    {
      "src": "assets/screenshots/screenshot.png",
      "sizes": "1280x720",
      "type": "image/png"
    }
